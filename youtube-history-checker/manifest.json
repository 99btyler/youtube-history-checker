{
	"name": "youtube-history-checker",
	"version": "1",
	"description": "Chrome extension that helps you start searching for a YouTube video in history",
	"permissions": ["declarativeContent"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "src/popup.html",
		"default_icon": {
			"16": "images/logo16.png",
			"32": "images/logo32.png",
			"48": "images/logo48.png",
			"128": "images/logo128.png"
		}
	},
	"icons": {
		"16": "images/logo16.png",
		"32": "images/logo32.png",
		"48": "images/logo48.png",
		"128": "images/logo128.png"
	},
	"manifest_version": 2
}